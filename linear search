#include<stdio.h>
int main()
{
	int i,n,a[50],key,flag=0;
	printf("enter n:");
	scanf("%d",&n);
	printf("enter array elements");
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	printf("enter the key elements to search");
	scanf("%d",&key);
	for(i=0;i<n;i++)
	{
		if(a[i]==key)
		{
			flag=1;
			break;
		}
	}
	if(flag==1)
	printf("the key is found at %d",i+1);
	else
	printf("the key is not found");
}
